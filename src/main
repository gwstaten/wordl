#include <iostream>
#include <algorithm>
#include <vector>
#include <fstream>

int main()
{
  std::ifstream fin("wordlists/11");
  std::ofstream fout("11uniqueLetters");
  std::string in;
  while(!fin.eof())
  {
    fin >> in;
    bool notDone = true;
    for(int i = 0; i < 11 && notDone; i++)
    {
      for(int j = 0; j < i && notDone; j++)
      {
        if(in.at(i) == in.at(j))
        {
          notDone = false;
        }
      }
    }
    if(notDone)
    {
      fout << in << std::endl;
      std::cout << in << std::endl;
    }
  }
}
